- name: gatlingUsers # NOWY PARAMETR
  type: string
  default: '10' # Domyślna wartość, zmień na odpowiednią
- name: simulationName # NOWY PARAMETR
  type: string
  default: 'DefaultSimulation' # Domyślna wartość, zmień na odpowiednią

  -e "GATLING_USERS=${{ parameters.gatlingUsers }}" \ # Zmienione na parametr potoku
        -e "SIMULATION_NAME=${{ parameters.simulationName }}" # Zmienione na parametr potoku
