declare namespace GatlingJavaExtensions {
    interface System {
        getProperty(key: string): string | null;
        setProperty(key: string, value: string): string | null;
    }
}

declare global {
    var GatlingJavaSystem: GatlingJavaExtensions.System;
    var Java: { type(className: string): any }; //Jeśli używasz Java.type gdzie indziej
}
