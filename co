function byteArrayToJsonString(byteArray: Uint8Array): string {
    // Konwersja tablicy bajt√≥w na Base64 string
    let base64String = "";
    for (let i = 0; i < byteArray.length; i++) {
        base64String += String.fromCharCode(byteArray[i]);
    }

    // Dekodowanie Base64 do JSON string
    const jsonString = base64.decode(base64String);

    return jsonString; // Zwraca JSON jako string
}
